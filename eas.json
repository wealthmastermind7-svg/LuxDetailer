{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.0-xcode-16.2"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "image": "macos-sequoia-15.0-xcode-16.2"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "",
        "ascAppId": "",
        "appleTeamId": ""
      }
    }
  }
}
