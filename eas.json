{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "image": "macos-sonoma-14.6-xcode-16.1",
        "resourceClass": "m-medium"
      },
      "android": {
        "image": "latest"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      },
      "ios": {
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "",
        "ascAppId": "",
        "appleTeamId": ""
      }
    }
  }
}
